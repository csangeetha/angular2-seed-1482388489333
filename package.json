{
  "name": "angular2-express-starter",
  "version": "1.0.0",
  "description": "Starter application of Angular2 on Express on Bluemix",
  "main": "gulpfile.js",
  "private": true,
  "scripts": {
    "start": "gulp build.dev && export env=dev && node ./server/bin/www",
    "build.dev": "typings install && gulp build.dev --env dev",
    "build.prod": "typings install && gulp build.prod --env prod",
    "tasks.list": "gulp --tasks-simple",
    "serve.dev.js.old": "export env=dev && node ./server/bin/www",
    "serve.prod.js.old": "export env=prod && node ./server/bin/www",
    "serve.dev.ts": "export env=dev && node ./dist/server/app",
    "serve.prod.ts": "export env=prod && node ./dist/server/app"
  },
  "author": "Ross Tang",
  "license": "MIT",
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "body-parser": "~1.13.2",
    "bootstrap": "^3.3.6",
    "c3": "^0.4.11-rc4",
    "chart.js": "^2.0.2",
    "cookie-parser": "~1.3.5",
    "d3": "^3.5.16",
    "debug": "^2.2.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "express": "^4.13.4",
    "font-awesome": "^4.6.1",
    "highcharts": "^4.2.4",
    "jquery": "^2.2.3",
    "jquery-ui": "^1.10.5",
    "jsonwebtoken": "^5.5.4",
    "moment": "^2.13.0",
    "morgan": "~1.6.1",
    "ng2-bootstrap": "1.0.13",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "serve-favicon": "~2.3.0",
    "systemjs": "0.19.24",
    "zone.js": "^0.6.10"
  },
  "devDependencies": {
    "async": "^2.0.0-rc.3",
    "browser-sync": "^2.12.3",
    "chalk": "^1.1.3",
    "concurrently": "^2.0.0",
    "connect-history-api-fallback": "^1.2.0",
    "del": "^2.2.0",
    "event-stream": "^3.3.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.1",
    "gulp-inject": "^4.0.0",
    "gulp-inline-ng2-template": "^1.1.3",
    "gulp-load-plugins": "^1.2.2",
    "gulp-minify": "0.0.9",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-template": "^4.0.0",
    "gulp-tslint": "^3.3.0",
    "gulp-typescript": "^2.13.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "merge-stream": "^1.0.0",
    "run-sequence": "^1.1.5",
    "slash": "^1.0.0",
    "systemjs-builder": "^0.15.15",
    "ts-node": "^0.7.2",
    "typescript": "^1.8.10",
    "typescript-node": "^0.1.3",
    "typescript-register": "^1.1.0",
    "typescript-require": "^0.2.9-1",
    "typings": "^0.7.12",
    "yargs": "^4.6.0"
  },
  "engines": {
    "node": "5.12.0",
    "npm": "3.8.5"
  }
}
